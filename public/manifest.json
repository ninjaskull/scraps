
{
  "manifest_version": 3,
  "name": "Scrapo",
  "version": "1.1",
  "description": "Extract publicly available lead data from LinkedIn Sales Navigator efficiently and export it in CSV format.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://www.linkedin.com/sales/lists/people"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/scrapo_128.png"
  },
  "icons": {
    "16": "images/scrapo_16.png",
    "48": "images/scrapo_48.png",
    "128": "images/scrapo_128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
